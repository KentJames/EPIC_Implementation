	file = {arXiv\:1009.0666 PDF:/Users/jameskent/Zotero/storage/9GYKV3KJ/Henning et al. - 2010 - The First Station of the Long Wavelength Array.pdf:application/pdf;arXiv.org Snapshot:/Users/jameskent/Zotero/storage/MZVNFV8T/1009.html:text/html}
