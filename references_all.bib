	pages = {545--554},
	file = {IEEE Xplore Abstract Record:/Users/jameskent/Zotero/storage/8PNDC4GL/7967145.html:text/html;Veenboer et al_2017_Image-Domain Gridding on Graphics Processors.pdf:/Users/jameskent/Google_Drive/PhD/Academic/Reading/Veenboer et al_2017_Image-Domain Gridding on Graphics Processors.pdf:application/pdf}
